# Le serveur doit suivre les liens symboliques :
Options +FollowSymlinks

# Activation du module de réécriture d'URL :
RewriteEngine on

# Client sans numéro de page :
RewriteRule ^Customers/$   Customers/View.php [L]
RewriteRule ^Customer/new$ Customers/Insert.php [L]

RewriteRule ^Customer/([0-9]+)  Customers/Update.php?id=$1 [L]